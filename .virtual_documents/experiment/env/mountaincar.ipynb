


import gymnasium as gym
env = gym.make('MountainCar-v0', render_mode='rgb_array')
obs, info = env.reset()
import mediapy

frames = []
c = 0
while True:
    c += 1
    # sample random action
    # action = env.action_space.sample()
    action = 1
    obs, rewards, terms, truncs, info = env.step(action)
    done = terms or truncs
